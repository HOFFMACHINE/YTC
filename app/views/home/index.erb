<div>
  <%= link_to 'index', root_path %>
</div>
<div>
  <p>Home</p>
  <p>Video</p>
<% if current_user %>
  <p>Profile</p>
<% end %>
</div>

<div>
<% if current_user %>
    <span><%= current_user.first_name %></span>
    <%= link_to 'Log Out', logout_path, method: :delete %>
<% else %>
    <%= link_to 'Google +', '/auth/google_oauth2' %>
    <%= link_to 'Twitter', '/auth/twitter' %>
    <%= link_to 'Facebook', '/auth/facebook' %>
<% end %>
</div>

